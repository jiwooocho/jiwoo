    "def sum1(a, b):\n",
    "    x = a + b\n",
    "    return x\n",
    "\n",
    "def sum2(*args):\n",
    "    x = 0\n",
    "    for i in args:\n",
    "        x += i\n",
    "    return x\n",
    "\n",
    "a = 5\n",
    "b = 3\n",
    "\n",
    "print(sum1(a, b))\n",
    "print(sum1(3, 5))\n",
    "print(sum2(1, 2, 3, 4, 5))\n",
    "print(sum2(2, 3.5, 10))"
   ]
